{
  "name": "jurinapse",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:snap": "node build-snap.js",
    "build:seo": "node build-seo.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "nodemon server.js",
    "server:quiet": "set LOG_LEVEL=minimal&& node server.js",
    "server:debug": "set LOG_LEVEL=debug&& node server.js",
    "server:silent": "set LOG_LEVEL=silent&& node server.js"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.561.0",
    "mongoose": "^8.16.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.1.10",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-helmet-async": "^2.0.5",
    "resend": "^6.2.0-preview-contacts.0",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^24.0.10",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.16",
    "axios": "^1.11.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite": "^7.2.7"
  },
  "reactSnap": {
    "include": [
      "/",
      "/feed",
      "/trending",
      "/decision",
      "/messaging",
      "/notifications",
      "/profile",
      "/folders",
      "/settings/blocked"
    ],
    "skipThirdPartyRequests": true,
    "cacheAjaxRequests": false,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "crawlFrom": "/",
    "waitFor": 2000,
    "removeBlobs": true,
    "sourceMaps": false,
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true
    }
  }
}
